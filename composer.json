{
    "name": "octaviancernat/laravel-virtualmin",
    "description": "A PHP Virtualmin API library for Laravel",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "library",
    "authors": [
        {
            "name": "octaviancernat",
            "email": "octavian.s.c@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "TheApp\\Virtualmin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheApp\\Virtualmin\\VirtualminServiceProvider"
            ],
            "aliases": {
                "Virtualmin": "TheApp\\Virtualmin\\Facades\\VirtualminApi"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/",
            "TheApp\\Virtualmin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse"
        ]
    }
}
