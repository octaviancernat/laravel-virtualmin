{
  "name": "ocs/laravel-virtualmin",
  "description": "A PHP Virtualmin API library for Laravel",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "authors": [
    {
      "name": "ocs",
      "email": "octavian.s.c@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "TheApp\\Virtualmin\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheApp\\VirtualminServiceProvider"
      ],
      "aliases": {
        "Virtualmin": "TheApp\\Virtualmin\\Facades\\Virtualmin"
      },
      "dont-discover": [
        "ocs/laravel-virtualmin"
      ]
    }
  }
}