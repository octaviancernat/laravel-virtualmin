{
    "name": "octaviancernat/laravel-virtualmin",
    "description": "A PHP Virtualmin API library for Laravel",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "library",
    "authors": [
        {
            "name": "octaviancernat",
            "email": "octavian.s.c@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "TheApp\\Virtualmin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheApp\\Virtualmin\\VirtualminServiceProvider"
            ],
            "aliases": {
                "Virtualmin": "TheApp\\Virtualmin\\Facades\\VirtualminApi"
            }
        }
    }
}
